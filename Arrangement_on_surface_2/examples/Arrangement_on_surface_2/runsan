cmake -S . -DCMAKE_BUILD_TYPE=Debug \
  -DCMAKE_CXX_FLAGS="-frounding-math -Wall -Wextra -Wpedantic -fsanitize=address -fno-omit-frame-pointer" \
  -DCMAKE_EXE_LINKER_FLAGS="-frounding-math -fsanitize=address"
cmake -S . -DCMAKE_BUILD_TYPE=Debug \
  -DCMAKE_CXX_FLAGS="-frounding-math -Wall -Wextra -Wpedantic -g -O0" \
  -DCMAKE_EXE_LINKER_FLAGS="-frounding-math"
cmake -j

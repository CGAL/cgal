# R3 Single-Threaded Version for Unix-like Systems

DESCRIPTION = Unix (single-threaded)
PROVIDES = QT
CXXFLAGS = 
LDFLAGS = 
LIBS = qt
REQUIRES = DOLLAR_QTDIR
INCOMPATIBLE = DOLLAR_WINDOWS_COMPILER
STDINCLDIRS = /usr/include/qt3:${QTDIR}/include
INCLTHING = qglobal.h
STDLIBDIRS = ${QTDIR}/lib64:${QTDIR}/lib
LIBTHING = libqt.\*

